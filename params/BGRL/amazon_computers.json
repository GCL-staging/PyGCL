{
    "learning_rate": 0.01,
    "hidden_dim": 128,
    "proj_dim": 128,
    "activation": "rrelu",
    "base_model": "GCNConv",
    "num_layers": 2,
    "augmentor1": {
        "drop_edge_prob": 0.6,
        "drop_feat_prob": 0.2
    },
    "augmentor2": {
        "drop_edge_prob": 0.3,
        "drop_feat_prob": 0.3
    },
    "loss": "bootstrap",
    "bootstrap": {
        "encoder_norm": "batch",
        "projector_norm": "batch",
        "predictor_norm": "batch",
        "momentum": 0.99
    },
    "patience": 2000,
    "num_epochs": 2000
}
