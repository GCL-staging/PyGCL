{
    "learning_rate": 0.0001,
    "weight_decay": 0.00001,
    "hidden_dim": 512,
    "proj_dim": 128,
    "batch_size": 200,
    "num_layers": 3,
    "activation": "hardtanh",
    "augmentor1": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.3,
        "drop_feat_prob": 0.1
    },
    "augmentor2": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.4,
        "drop_feat_prob": 0.5
    },
    "loss": "bootstrap",
    "bootstrap": {
        "encoder_norm": "batch",
        "projector_norm": "batch",
        "predictor_norm": "batch",
        "momentum": 0.99
    },
    "num_epochs": 100
}