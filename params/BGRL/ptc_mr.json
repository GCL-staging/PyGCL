{
    "learning_rate": 0.001,
    "weight_decay": 5e-8,
    "hidden_dim": 256,
    "proj_dim": 128,
    "batch_size": 8,
    "num_layers": 3,
    "activation": "hardtanh",
    "augmentor1": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.2,
        "drop_feat_prob": 0
    },
    "augmentor2": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.3,
        "drop_feat_prob": 0.3
    },
    "loss": "bootstrap",
    "bootstrap": {
        "encoder_norm": "batch",
        "projector_norm": "batch",
        "predictor_norm": "batch",
        "momentum": 0.99
    },
    "num_epochs": 200
}