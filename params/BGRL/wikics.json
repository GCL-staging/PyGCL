{
    "learning_rate": 0.01,
    "weight_decay": 5e-8,
    "hidden_dim": 256,
    "proj_dim": 256,
    "activation": "rrelu",
    "augmentor1": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.5,
        "drop_feat_prob": 0.1
    },
    "augmentor2": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.4,
        "drop_feat_prob": 0
    },
    "loss": "bootstrap",
    "bootstrap": {
        "encoder_norm": "batch",
        "projector_norm": "batch",
        "predictor_norm": "batch",
        "momentum": 0.99
    },
    "patience": 200,
    "num_epochs": 2000,
    "dropout": 0.2
}
