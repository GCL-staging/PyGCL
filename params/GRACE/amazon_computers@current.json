{
    "learning_rate": 0.01,
    "hidden_dim": 128,
    "proj_dim": 128,
    "activation": "rrelu",
    "base_model": "GCNConv",
    "num_layers": 2,
    "num_epochs": 2000,
    "patience": 200,
    "augmentor1": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.6,
        "drop_feat_prob": 0.2
    },
    "augmentor2": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.3,
        "drop_feat_prob": 0.3
    },
    "loss": "infonce",
    "infonce": {
        "tau": 0.2
    },
    "debiased_infonce": {
        "tau": 0.2,
        "tau_plus": 0.2
    },
    "hardness_infonce": {
        "tau": 0.2,
        "tau_plus": 0.2,
        "beta": 10.0
    },
    "ring_loss": {
        "tau": 0.2
    },
    "batch_norm": false
}
