{
    "dataset": "Amazon-Computers",
    "opt": {
        "learning_rate": 0.01,
        "patience": 200,
        "num_epochs": 2000
    },
    "encoder": {
        "hidden_dim": 128,
        "proj_dim": 128,
        "activation": "rrelu"
    },
    "augmentor1": {
        "drop_edge_prob": 0.6,
        "drop_feat_prob": 0.2
    },
    "augmentor2": {
        "drop_edge_prob": 0.3,
        "drop_feat_prob": 0.3
    },
    "obj": {
        "loss": "infonce",
        "infonce": {
            "tau": 0.2
        }
    }
}
