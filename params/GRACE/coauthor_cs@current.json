{
    "learning_rate": 0.0005,
    "hidden_dim": 256,
    "proj_dim": 256,
    "activation": "rrelu",
    "base_model": "GCNConv",
    "num_layers": 2,
    "num_epochs": 1000,
    "patience": 200,
    "augmentor1": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.3,
        "drop_feat_prob": 0.3
    },
    "augmentor2": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.2,
        "drop_feat_prob": 0.4
    },
    "loss": "subsampling_infonce",
    "infonce": {
        "tau": 0.4
    },
    "debiased_infonce": {
        "tau": 0.4,
        "tau_plus": 0.2
    },
    "hardness_infonce": {
        "tau": 0.4,
        "tau_plus": 0.2,
        "beta": 10.0
    },
    "subsampling_infonce": {
        "tau": 0.4,
        "negative_size": 1000
    },
    "ring_loss": {
        "tau": 0.4
    },
    "batch_norm": false
}