{
    "learning_rate": 0.01,
    "hidden_dim": 128,
    "proj_dim": 64,
    "activation": "rrelu",
    "num_epochs": 1500,
    "patience": 1500,
    "augmentor1": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.4,
        "drop_feat_prob": 0.1
    },
    "augmentor2": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.1,
        "drop_feat_prob": 0.4
    },
    "loss": "infonce",
    "infonce": {
        "tau": 0.5
    },
    "debiased_infonce": {
        "tau": 0.5,
        "tau_plus": 0.2
    },
    "hardness_infonce": {
        "tau": 0.5,
        "tau_plus": 0.2,
        "beta": 10.0
    },
    "ring_loss": {
        "tau": 0.5
    },
    "batch_norm": false
}