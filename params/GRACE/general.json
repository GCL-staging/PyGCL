{
    "dataset": "WikiCS",
    "opt": {
        "learning_rate": 0.01,
        "weight_decay": 5e-8,
        "num_epochs": 10000,
        "patience": 300,
        "reduce_lr_patience": 100
    },
    "encoder": {
        "hidden_dim": 256,
        "proj_dim": 256,
        "activation": "prelu",
        "conv": "GCNConv"
    },
    "augmentor1": {
        "scheme": "ER+FM",
        "ER:prob": 0.5,
        "FM:prob": 0.1
    },
    "augmentor2": {
        "scheme": "ER+FM",
        "ER:prob": 0.5,
        "FM:prob": 0.1
    },
    "obj": {
        "loss": "infonce",
        "infonce:tau": 0.7
    }
}