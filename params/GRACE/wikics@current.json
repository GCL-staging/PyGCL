{
    "learning_rate": 0.01,
    "weight_decay": 5e-8,
    "hidden_dim": 256,
    "proj_dim": 256,
    "activation": "rrelu",
    "augmentor1": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.5,
        "drop_feat_prob": 0.1
    },
    "augmentor2": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.4,
        "drop_feat_prob": 0
    },
    "loss": "infonce",
    "infonce": {
        "tau": 0.7
    },
    "num_epochs": 1800
}
