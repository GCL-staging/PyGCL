{
    "learning_rate": 0.01,
    "weight_decay": 5e-8,
    "hidden_dim": 256,
    "proj_dim": 256,
    "activation": "rrelu",
    "augmentor1": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.5,
        "drop_feat_prob": 0.1
    },
    "augmentor2": {
        "scheme": "ER+FM",
        "drop_edge_prob": 0.4,
        "drop_feat_prob": 0
    },
    "loss": "ring_loss",
    "infonce": {
        "tau": 0.7
    },
    "debiased_infonce": {
        "tau": 0.7,
        "tau_plus": 0.2
    },
    "hardness_infonce": {
        "tau": 0.7,
        "tau_plus": 0.2,
        "beta": 10.0
    },
    "ring_loss": {
        "tau": 0.7
    },
    "num_epochs": 1800,
    "patience": 50,
    "num_layers": 2,
    "batch_norm": false
}
