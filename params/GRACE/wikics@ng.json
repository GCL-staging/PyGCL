{
    "dataset": "WikiCS",
    "opt:learning_rate": 0.01,
    "opt:weight_decay": 5e-8,
    "opt:num_epochs": 10000,
    "encoder:hidden_dim": 256,
    "encoder:proj_dim": 256,
    "encoder:activation": "rrelu",
    "encoder:conv": "GCNConv",
    "augmentor1": {
        "scheme": "ER+FM",
        "ER:prob": 0.5,
        "FM:prob": 0.1
    },
    "augmentor2": {
        "scheme": "ER+FM",
        "ER:prob": 0.5,
        "FM:prob": 0.1
    },
    "obj:loss": "infonce",
    "obj:infonce:tau": 0.7
}