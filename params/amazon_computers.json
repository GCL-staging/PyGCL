{
    "dataset": "Amazon-Computers",
    "mode": "L2L",
    "opt:learning_rate": 0.01,
    "opt:weight_decay": 5e-8,
    "opt:num_epochs": 2000,
    "opt": {
        "batch_size": 200,
        "patience": 1000,
        "reduce_lr_patience": -1
    },
    "encoder:hidden_dim": 128,
    "encoder:proj_dim": 128,
    "encoder:num_layers": 2,
    "encoder:conv": "GCNConv",
    "encoder:activation": "rrelu",
    "augmentor1:scheme": "ER+FM",
    "augmentor2:scheme": "ER+FM",
    "augmentor1:ER:prob": 0.6,
    "augmentor2:ER:prob": 0.3,
    "augmentor1:FM:prob": 0.2,
    "augmentor2:FM:prob": 0.3,
    "obj:loss": "infonce",
    "obj:infonce:tau": 0.2
}