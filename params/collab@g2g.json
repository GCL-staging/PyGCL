{
    "dataset": "COLLAB",
    "mode": "G2G",
    "opt:learning_rate": 0.001,
    "opt:weight_decay": 0.0001,
    "opt:num_epochs": 2000,
    "opt": {
        "batch_size": 80,
        "patience": 200,
        "reduce_lr_patience": 100
    },
    "encoder:hidden_dim": 32,
    "encoder:proj_dim": 512,
    "encoder:num_layers": 2,
    "encoder:conv": "GINConv",
    "encoder:activation": "hardtanh",
    "augmentor1:scheme": "ND+FM",
    "augmentor2:scheme": "ND+FM",
    "augmentor1:ND:prob": 0.5,
    "augmentor2:ND:prob": 0.4,
    "augmentor1:FM:prob": 0.1,
    "augmentor2:FM:prob": 0.3,
    "obj:loss": "infonce",
    "obj:infonce:tau": 0.1
}