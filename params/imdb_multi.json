{
    "dataset": "IMDB-MULTI",
    "mode": "L2L",
    "opt:learning_rate": 0.001,
    "opt:weight_decay": 1e-9,
    "opt:num_epochs": 300,
    "opt": {
        "batch_size": 200,
        "patience": 500,
        "reduce_lr_patience": -1
    },
    "encoder:hidden_dim": 256,
    "encoder:proj_dim": 512,
    "encoder:num_layers": 2,
    "encoder:conv": "GINConv",
    "encoder:activation": "hardtanh",
    "augmentor1:scheme": "ND+FM",
    "augmentor2:scheme": "ND+FM",
    "augmentor1:ND:prob": 0.4,
    "augmentor2:ND:prob": 0.2,
    "augmentor1:FM:prob": 0.3,
    "augmentor2:FM:prob": 0.2,
    "obj:loss": "infonce",
    "obj:infonce:tau": 0.9
}