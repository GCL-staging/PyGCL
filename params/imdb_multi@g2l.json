{
    "dataset": "IMDB-MULTI",
    "mode": "G2L",
    "opt:learning_rate": 0.0001,
    "opt:weight_decay": 1e-8,
    "opt:num_epochs": 300,
    "opt": {
        "batch_size": 200,
        "patience": 200,
        "reduce_lr_patience": 100
    },
    "encoder:hidden_dim": 256,
    "encoder:proj_dim": 128,
    "encoder:num_layers": 2,
    "encoder:conv": "GINConv",
    "encoder:activation": "hardtanh",
    "encoder:shared_proj": true,
    "augmentor1:scheme": "ND+FM",
    "augmentor2:scheme": "ND+FM",
    "augmentor1:ND:prob": 0.5,
    "augmentor2:ND:prob": 0.4,
    "augmentor1:FM:prob": 0.5,
    "augmentor2:FM:prob": 0.1,
    "obj:loss": "infonce",
    "obj:infonce:tau": 0.8
}