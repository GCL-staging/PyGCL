{
    "dataset": "NCI1",
    "mode": "L2L",
    "opt:learning_rate": 0.0001,
    "opt:weight_decay": 1e-7,
    "opt:num_epochs": 1800,
    "opt": {
        "batch_size": 200,
        "patience": 500,
        "reduce_lr_patience": -1
    },
    "encoder:hidden_dim": 512,
    "encoder:proj_dim": 512,
    "encoder:num_layers": 6,
    "encoder:conv": "GINConv",
    "encoder:activation": "relu",
    "augmentor1:scheme": "ND+FM",
    "augmentor2:scheme": "ND+FM",
    "augmentor1:ND:prob": 0.3,
    "augmentor2:ND:prob": 0.4,
    "augmentor1:FM:prob": 0.1,
    "augmentor2:FM:prob": 0.1,
    "obj:loss": "infonce",
    "obj:infonce:tau": 0.3
}