{
    "dataset": "NCI1",
    "mode": "L2L",
    "opt:learning_rate": 0.0005,
    "opt:weight_decay": 5e-8,
    "opt:num_epochs": 1800,
    "opt": {
        "batch_size": 200,
        "patience": 200,
        "reduce_lr_patience": 100
    },
    "encoder:hidden_dim": 512,
    "encoder:proj_dim": 128,
    "encoder:num_layers": 5,
    "encoder:conv": "GINConv",
    "encoder:activation": "relu",
    "augmentor1:scheme": "ND+FM",
    "augmentor2:scheme": "ND+FM",
    "augmentor1:ND:prob": 0.1,
    "augmentor2:ND:prob": 0.1,
    "augmentor1:FM:prob": 0.0,
    "augmentor2:FM:prob": 0.0,
    "obj:infonce:tau": 0.7
}
