{
    "dataset": "ogbn-arxiv",
    "mode": "L2L",
    "opt": {
        "learning_rate": 0.01,
        "weight_decay": 1e-7,
        "num_epochs": 1,
        "batch_size": 500,
        "patience": 500,
        "reduce_lr_patience": -1
    },
    "encoder:hidden_dim": 256,
    "encoder:proj_dim": 256,
    "encoder:num_layers": 2,
    "encoder:conv": "GCNConv",
    "encoder:activation": "prelu",
    "augmentor1": {
        "scheme": "ER+FM",
        "ER:prob": 0.1,
        "FM:prob": 0.4
    },
    "augmentor2": {
        "scheme": "ER+FM",
        "ER:prob": 0.1,
        "FM:prob": 0.5
    }
}