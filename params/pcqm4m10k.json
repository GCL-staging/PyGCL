{
    "dataset": "PCQM4M-10K",
    "mode": "L2L",
    "opt:learning_rate": 0.01,
    "opt:weight_decay": 1e-8,
    "opt:num_epochs": 800,
    "opt": {
        "batch_size": 256,
        "patience": 500,
        "reduce_lr_patience": -1
    },
    "encoder:hidden_dim": 256,
    "encoder:proj_dim": 64,
    "encoder:num_layers": 3,
    "encoder:conv": "GINEConv",
    "encoder:activation": "elu",
    "augmentor1:scheme": "ER+FM",
    "augmentor2:scheme": "ER+FM",
    "augmentor1:ER:prob": 0.1,
    "augmentor2:ER:prob": 0.4,
    "augmentor1:FM:prob": 0.2,
    "augmentor2:FM:prob": 0.2,
    "obj:loss": "infonce",
    "obj:infonce:tau": 0.9
}