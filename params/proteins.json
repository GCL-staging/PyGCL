{
    "dataset": "PROTEINS",
    "mode": "L2L",
    "opt:learning_rate": 0.01,
    "opt:weight_decay": 1e-7,
    "opt:num_epochs": 1800,
    "opt": {
        "batch_size": 200,
        "patience": 500,
        "reduce_lr_patience": -1
    },
    "encoder:hidden_dim": 128,
    "encoder:proj_dim": 128,
    "encoder:num_layers": 4,
    "encoder:conv": "GINConv",
    "encoder:activation": "hardtanh",
    "augmentor1:scheme": "ND+FM",
    "augmentor2:scheme": "ND+FM",
    "augmentor1:ND:prob": 0.5,
    "augmentor2:ND:prob": 0.4,
    "augmentor1:FM:prob": 0.0,
    "augmentor2:FM:prob": 0.5,
    "obj:loss": "infonce",
    "obj:infonce:tau": 0.2
}