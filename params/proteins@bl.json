{
    "dataset": "PROTEINS",
    "mode": "G2G",
    "opt": {
        "learning_rate": 0.001,
        "weight_decay": 1e-7,
        "num_epochs": 300,
        "batch_size": 200,
        "patience": 500,
        "reduce_lr_patience": -1
    },
    "encoder:hidden_dim": 128,
    "encoder:proj_dim": 128,
    "encoder:num_layers": 2,
    "encoder:conv": "GINConv",
    "encoder:activation": "hardtanh",
    "augmentor1:scheme": "ND+FM",
    "augmentor2:scheme": "ND+FM",
    "augmentor1": {
        "scheme": "ND+FM",
        "ND:prob": 0.1,
        "FM:prob": 0.4
    },
    "augmentor2": {
        "scheme": "ND+FM",
        "ND:prob": 0.1,
        "FM:prob": 0.5
    }
}