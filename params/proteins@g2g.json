{
    "dataset": "PROTEINS",
    "mode": "G2G",
    "opt:learning_rate": 0.0001,
    "opt:weight_decay": 1e-7,
    "opt:num_epochs": 800,
    "opt": {
        "batch_size": 80,
        "patience": 200,
        "reduce_lr_patience": 100
    },
    "encoder:hidden_dim": 128,
    "encoder:proj_dim": 64,
    "encoder:num_layers": 4,
    "encoder:conv": "GINConv",
    "encoder:activation": "leakyrelu",
    "augmentor1:scheme": "ND+FM",
    "augmentor2:scheme": "ND+FM",
    "augmentor1:ND:prob": 0.5,
    "augmentor2:ND:prob": 0.5,
    "augmentor1:FM:prob": 0.4,
    "augmentor2:FM:prob": 0.5,
    "obj:loss": "infonce",
    "obj:infonce:tau": 0.1
}