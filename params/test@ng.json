{
  "dataset": "NCI1",
  "opt": {
    "learning_rate": 0.01,
    "weight_decay": 5e-8,
    "num_epochs": 200,
    "patience": 300,
    "reduce_lr_patience": 100,
    "batch_size": 200
  },
  "encoder": {
    "hidden_dim": 256,
    "proj_dim": 256,
    "activation": "prelu",
    "conv": "GINConv"
  },
  "augmentor1": {
    "scheme": "ND+FM",
    "ND:prob": 0.4,
    "FM:prob": 0.1
  },
  "augmentor2": {
    "scheme": "ND+FM",
    "ND:prob": 0.4,
    "FM:prob": 0.1
  },
  "obj": {
    "loss": "infonce",
    "infonce:tau": 0.7
  }
}
